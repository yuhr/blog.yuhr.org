{
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": { "source.fixAll.eslint": true },
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"files.removeFinalNewlines": true,
	"files.associations": {
		".*rc": "json",
		".eslintrc": "jsonc",
		"fsroute": "typescript"
	},
	"typescript.tsdk": "node_modules/typescript/lib"
}